	// GPIOA_4 USB LOCAL RESRV_nPWR-OFF
	gpio-line-names =
		/* GPIOZ */
		"", "", "", "", "", // 0 - 4
		"", "", "", "", "", // 5 - 9
		"UserButton", // 10
		/* BOOT */
		"", "", "", "", "", // 11 - 15
		"", "", "", "", "EMMC_RST", // 16 - 20
		"", "", "", "", "", // 21 - 25
		/* GPIOA */
		//0 538    1 539      2  540     	3 541
		"LedRed", "LedBlue", "CPU2_T_DISABLE", "CPU1_OFF", // 26 - 30
		//4 542      543 544 545 546
		"USB_LOCAL", "", "", "", "", // 31 - 35
		//9 547     548     549     550     551     552     553     554     555     556     557
		"USBHUB_RESET", "", "", "", "", // 36 - 40
		"BT04_PWROFF", "BT04_WORK", "BT04_KEY", "BT04_RESET", "BT04_LINK", // 41 - 45
		"", "", // 46 - 47
		/* GPIOX */
		"WIFI_D0", "WIFI_D1", "WIFI_D2", // 47 - 49
		"WIFI_D3", "WIFI_CLK", "WIFI_CMD", "WIFI_WAKEHOST", "WIFI_PWR_EN", // 50 - 54
		"", "", "", "", "", // 55 - 59
		"", "", "", "", "", // 60 - 64
		"", "", "WIFI_32K_LPO", "", "", // 65 - 69
		/* GPIOY */
		"", "", "", "", "", // 70 - 74
		"", "", "", "", "", // 75 - 79
		"", "", "", "", "", // 80 - 84
		""; // 85
};

&gpio_ao {
	gpio-line-names =
		"CONSOLE_TX", "CONSOLE_RX", "VDDCPU_PWM", "GPIOAO_3", "UART_AO_TX", // 0 - 4
		"UART_AO_RX", "GPIOAO_6", "GPIOAO_7", "BT_WAKE_HOST", "VDDEE_PWM", // 5 - 9
		"GPIOAO_10", "GPIOAO_11", "GPIOAO_12", "GPIOAO_13", "VDDCPU_EN"; // 10 - 14
};

&cpu0 {
	#cooling-cells = <2>;
	cpu-supply = <&vddcpu>;
};

&cpu1 {
	#cooling-cells = <2>;
	cpu-supply = <&vddcpu>;
};

&cpu2 {
	#cooling-cells = <2>;
	cpu-supply = <&vddcpu>;
};

&cpu3 {
	#cooling-cells = <2>;
	cpu-supply = <&vddcpu>;
};

&pwm_AO_ab {
	pinctrl-0 = <&pwm_ao_a_pins>;
	pinctrl-names = "default";
	clocks = <&xtal>;
	clock-names = "clkin1";
	status = "okay";
};

&pwm_AO_cd {
	pinctrl-0 = <&pwm_ao_d_pins>;
	pinctrl-names = "default";
	clocks = <&xtal>;
	clock-names = "clkin1";
	status = "okay";
};

&pinctrl_aobus {
	pwm_ao_a_pins: pwm_ao_a {
		mux {
			groups = "pwm_ao_a";
			function = "pwm_ao_a";
			bias-disable;
		};
	};

	pwm_ao_d_pins: pwm_ao_d {
		mux {
			groups = "pwm_ao_d";
			function = "pwm_ao_d";
			bias-disable;
		};
	};
};

&dwc2 {
	vusb_a-supply = <&vddio_ao18>;
	vusb_d-supply = <&vddao_3v3>;
};
